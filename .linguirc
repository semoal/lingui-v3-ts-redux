{
 "catalogs": [{
   "path": "<rootDir>/src/locales/{locale}/messages",
   "include": ["<rootDir>/src"],
   "exclude": ["**/node_modules/**"]
 }],
 "compileNamespace": "cjs",
 "format": "po",
 "orderBy": "messageId",
 "locales": ["en"],
 "runtimeConfigModule": ["@lingui/core", "i18n"],
 "sourceLocale": "en",
}